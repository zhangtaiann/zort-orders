{
  "rewrites": [
    {
      "source": "/products",
      "destination": "/products.html"
    },
    {
      "source": "/product",
      "destination": "/product.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}